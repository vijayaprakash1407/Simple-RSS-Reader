<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.title %></td>
    <td><%= feed.url %></td>
    <td><%= feed.created_at %></td>
    <td><%= feed.updated_at %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
<%= link_to 'Show Feed Entries', :controller=>"entries",:action=>"index" %>